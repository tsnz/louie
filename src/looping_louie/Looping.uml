        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaNUUGZuEeeC7M0ZKcTJ6A" name="ExtendedGame" specification="_gUpe4GZuEeeC7M0ZKcTJ6A">
