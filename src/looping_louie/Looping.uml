        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaaIoGZuEeeC7M0ZKcTJ6A" name="run" specification="_gRrqcGZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>try {&#xD;
			this.gameReadyToStartLatch.await();&#xD;
		} catch (InterruptedException e) {&#xD;
			// can be ignored since latch wont be interrupted&#xD;
		}&#xD;
&#xD;
		try {&#xD;
			while (!this.thread.isInterrupted()) {&#xD;
&#xD;
				String bluetoothIn = this.bluetoothInStream.readUTF();&#xD;
&#xD;
				// check if game exists&#xD;
				if (this.game != null) {&#xD;
&#xD;
					switch (bluetoothIn) {&#xD;
					case &quot;ENTER&quot;:&#xD;
						this.game.toggleMotorDirection();&#xD;
						break;&#xD;
					case &quot;DOWN&quot;:&#xD;
						this.game.decreaseMotorSpeed();&#xD;
						break;&#xD;
					case &quot;UP&quot;:&#xD;
						this.game.increaseMotorSpeed();&#xD;
						break;&#xD;
					default:&#xD;
						break;&#xD;
					}&#xD;
&#xD;
				}&#xD;
&#xD;
			}&#xD;
		} catch (IOException e) {&#xD;
			// can be ignored&#xD;
		}&#xD;
		return;&#xD;
&#xD;
	</body>
        </ownedBehavior>
