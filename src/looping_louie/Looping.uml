        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaLfImZuEeeC7M0ZKcTJ6A" name="displayLifesForPlayer" specification="_gQYC4GZuEeeC7M0ZKcTJ6A">
