		case 0: // red player&#xD;
			LCD.drawInt(lifes, 0, 3);&#xD;
			break;&#xD;
&#xD;
		case 2: // black player&#xD;
			LCD.drawInt(lifes, 16, 3);&#xD;
			break;&#xD;
&#xD;
		default:&#xD;
			break;&#xD;
		}&#xD;
	</body>
