        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaRlwGZuEeeC7M0ZKcTJ6A" name="run" specification="_gQy5oGZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>this.display.clearDisplay();&#xD;
		this.setupAdditionalSensors();&#xD;
		this.setupGame();&#xD;
		this.begin();&#xD;
		try {&#xD;
			this.gameFinishedLatch.await();&#xD;
		} catch (InterruptedException e) {&#xD;
			// interruption not possible since this is the main thread&#xD;
		}&#xD;
		this.cleanup();&#xD;
		return;&#xD;
	</body>
