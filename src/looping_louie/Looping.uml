        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaN7YGZuEeeC7M0ZKcTJ6A" name="setupGame" specification="_gQf-sGZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>this.motor_moving_forward = true;&#xD;
		this.motor.setSpeed(this.configuration.getSpeed());&#xD;
		this.motor.setAcceleration(180);&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaN7YWZuEeeC7M0ZKcTJ6A" name="setupAdditionalSensors" specification="_gQhM0mZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>if (this.configuration.getRandomDirectionChange()) {&#xD;
			Sensor sensor2 = new TimeSensor(this, this.gameReadyToStartLatch, () -> this.toggleMotorDirection());&#xD;
			this.additionalSensors.add(sensor2);&#xD;
		}&#xD;
		Sensor sensor3 = new TimeSensor(this, this.gameReadyToStartLatch, () -> this.newRandomSpeed());&#xD;
		this.additionalSensors.add(sensor3);&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaN7YmZuEeeC7M0ZKcTJ6A" name="toggleMotorDirection" specification="_gQia8mZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>// check motor direction and change direction to opposite&#xD;
		if (this.motor_moving_forward == true) {&#xD;
			this.motor.forward();&#xD;
			this.motor_moving_forward = false;&#xD;
		} else {&#xD;
			this.motor.backward();&#xD;
			this.motor_moving_forward = true;&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaOicGZuEeeC7M0ZKcTJ6A" name="newRandomSpeed" specification="_gQjCAGZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>Random randomGenerator = new Random();&#xD;
		// new random speed using the given offsets&#xD;
		// subtract min_speed to prevent actual speed exceeding max_speed&#xD;
		int randomSpeed = randomGenerator.nextInt(Configuration.MAX_MOTOR_SPEED - Configuration.MIN_MOTOR_SPEED);&#xD;
		// add min_speed to generated speed&#xD;
		this.motor.setSpeed(randomSpeed + Configuration.MIN_MOTOR_SPEED);&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaOicWZuEeeC7M0ZKcTJ6A" name="cleanup" specification="_gQjCAWZuEeeC7M0ZKcTJ6A">
          <language>java</language>
          <body>this.motorListener.stop();&#xD;
		this.motor.close();&#xD;
		// stop all light sensors&#xD;
		for (Sensor sensor : lightSensors) {&#xD;
			sensor.stop();&#xD;
		}&#xD;
		// stop all additional sensors&#xD;
		for (Sensor sensor : additionalSensors) {&#xD;
			sensor.stop();&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
