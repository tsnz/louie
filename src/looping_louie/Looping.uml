			LCD.drawInt(lifes, DISPLAY_WIDTH / 2 - 1, 1);&#xD;
			break;&#xD;
&#xD;
		case 1: // blue player&#xD;
			LCD.drawInt(lifes, DISPLAY_WIDTH / 2 - 1, 6);&#xD;
			break;&#xD;
