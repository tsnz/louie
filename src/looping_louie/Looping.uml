        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_gaLfIGZuEeeC7M0ZKcTJ6A" name="displayInitialLifes" specification="_gQXb0GZuEeeC7M0ZKcTJ6A">
